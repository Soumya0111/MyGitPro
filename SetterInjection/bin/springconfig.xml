<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:context="http://www.springframework.org/schema/context"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation=" http://www.springframework.org/schema/beans 
	http://www.springframework.org/schema/beans/spring-beans.xsd
	http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd">

	<bean id="s1" class="com.setter.Student">
	<property name="id" value="1"></property>
	<property name="name" value="ravi"></property>
	<property name="marks" value="89.00"></property>
	</bean>

	<bean id="s2" class="com.setter.Student">
	<property name="id" value="2"></property>
	<property name="name" value="teja"></property>
	<property name="marks" value="90.00"></property>
	</bean>
</beans>